                        <castList>
                            <head type="h4" xml:id="tg77.2.1">Personen.</head>
                            <castGroup>
                                <castItem rend="zenoPLm0n4" xml:id="tg77.2.2">Ein Ehepaar und ein Kind.</castItem>
                                <lb xml:id="tg77.2.3"/>
                                <castItem rend="zenoPLm0n4" xml:id="tg77.2.4">Ein Chauffeur und ein Hund.</castItem>
